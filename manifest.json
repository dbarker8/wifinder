{
  "manifest_version": 2,
  "name": "WiFinder for Airbnb",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["*://*.airbnb.com/rooms/*"],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "modal.html",
    "styles.css",
    "content.js"
]
}