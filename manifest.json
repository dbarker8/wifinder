{
  "manifest_version": 2,
  "name": "WiFinder for Airbnb",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["*://*.airbnb.com/rooms/*"],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "modal.html",
    "styles.css",
    "content.js"
],
"permissions": [
  "*://vy1fbiczn2.execute-api.us-east-1.amazonaws.com/*",
  "webRequest"
]
}